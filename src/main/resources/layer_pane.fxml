<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<fx:root styleClass="pane" stylesheets="@styling/stylesheet.css" type="AnchorPane" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
  <children>
    <StackPane fx:id="stackPane" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
      <children>
        <ScrollPane>
          <content>
            <VBox fx:id="layerItemVBox" alignment="TOP_RIGHT" spacing="5.0">
                     <padding>
                        <Insets left="5.0" />
                     </padding></VBox>
          </content>
        </ScrollPane>
            <AnchorPane fx:id="emptyLayerPane" StackPane.alignment="CENTER">
               <children>
            <Label alignment="CENTER" text="Layer Manager" textAlignment="CENTER">
              <font>
                <Font size="36.0" />
              </font>
            </Label>
               </children>
            </AnchorPane>
      </children>
    </StackPane>
  </children>
</fx:root>
